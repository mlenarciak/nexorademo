{
  "name": "@repo/database",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "analyze": "prisma generate --no-hints --schema=./prisma/schema.prisma",
    "build": "prisma generate --no-hints --schema=./prisma/schema.prisma",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "tsx src/seed.ts",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "db:reset": "prisma migrate reset --force --schema=./prisma/schema.prisma"
  },
  "prisma": {
    "seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "6.16.3",
    "@prisma/client": "6.16.3",
    "@t3-oss/env-nextjs": "^0.13.8",
    "server-only": "^0.0.1",
    "undici": "^7.16.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "24.7.0",
    "@types/ws": "^8.18.1",
    "bufferutil": "^4.0.9",
    "prisma": "6.16.3",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
